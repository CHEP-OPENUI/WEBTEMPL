<!------------ MiniButton: standard text-based buttons -------------->
<swe:htmltype name="MiniButton">
   <span class="minibuttonOn">
		<nobr><swe:this property="Data" type="Button" /></nobr>
   </span>
</swe:htmltype>

<!------------ MiniButton (Disabled) -------------->
<swe:htmltype name="MiniButton" state="Disabled">
   <span class="minibuttonOff">
		<nobr><swe:this property="Data" type="Text" /></nobr>
   </span>
</swe:htmltype>
<!------------------------------------------------->

