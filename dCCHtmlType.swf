<!------------ Rich Text Component (RTC) -------------->
<swe:htmltype name="RTC_IO">
	<script>rtc_writeLaunchPad(document);</script>
</swe:htmltype>

<swe:htmltype name="RTCEmbedded">
	<script>createRTC(0,1,document)</script>
	<swe:this property="Data" type="TextArea"/>
	<script>createRTC(1,1,document)</script>
</swe:htmltype>

<swe:htmltype name="RTCEmbedded" mode="Base">
	<script>createRTC(0,3,document)</script>
	<swe:this property="Data" type="TextArea"/>
	<script>createRTC(1,3,document)</script>
</swe:htmltype>

<swe:htmltype name="RTCPopup">
	<script>createRTC(0,2,document)</script>
	<swe:this property="Data" type="TextArea"/>
	<script>createRTC(1,2,document)</script>
</swe:htmltype>

<swe:htmltype name="RTCPopup" mode="Base">
	<script>createRTC(0,3,document)</script>
	<swe:this property="Data" type="TextArea"/>
	<script>createRTC(1,3,document)</script>
</swe:htmltype>

<swe:htmltype name="RTCEmbeddedLinkField">
	<script>createRTCLinkCtl(0,document)</script>
	<div class="mceLabel"><swe:this property="DisplayName"/>:</div><swe:this property="Data" type="Text"/>
	<script>createRTCLinkCtl(1,document)</script>
</swe:htmltype>

<swe:htmltype name="RTCEmbeddedLinkField" mode="Base"></swe:htmltype>

<swe:htmltype name="RTCEmbeddedLinkField" mode="Query"></swe:htmltype>

