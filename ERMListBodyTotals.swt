      </swe:control>
   </tr>

<!-- ---------- List Field Values 4-column fields ------------->
   <swe:for-each startValue="500" count="5" iteratorName="currentId">
      <tr>
         <swe:control id="swe:currentId">
	         <td><div class="mceLabel" align="swe:this.TextAlignmentLabel">
		         <swe:this property="DisplayName" hintText="Label"/>:
		         <swe:this property="RequiredIndicator" hintText="Required"/>
	            </div>
	         </td>
	         <td><div class="mceField">
	  	         <swe:this property="FormattedHtml" hintText="Field" hintMapType="Hyperlinked Item"/>
               </div>
            </td>
         </swe:control> 

         <swe:control id="swe:currentId+10">
      	   <td><div class="mceLabel" align="swe:this.TextAlignmentLabel">
		         <swe:this property="DisplayName" hintText="Label"/>:
		         <swe:this property="RequiredIndicator" hintText="Required"/>
	            </div>
	         </td>
	         <td><div class="mceField">
	  	         <swe:this property="FormattedHtml" hintText="Field" hintMapType="Hyperlinked Item"/>
	            </div>
	         </td>
         </swe:control>
         
         <swe:control id="swe:currentId+20">
	         <td><div class="mceLabel" align="swe:this.TextAlignmentLabel">
		         <swe:this property="DisplayName" hintText="Label"/>:
		         <swe:this property="RequiredIndicator" hintText="Required"/>
	            </div>
	         </td>
               <td><div class="mceField">
	  	         <swe:this property="FormattedHtml" hintText="Field" hintMapType="Hyperlinked Item"/>
	            </div>
	         </td>
         </swe:control> 
         
         <swe:control id="swe:currentId+30">
	         <td><div class="mceLabel" align="swe:this.TextAlignmentLabel">
		         <swe:this property="DisplayName" hintText="Label"/>:
		         <swe:this property="RequiredIndicator" hintText="Required"/>
	            </div>
	         </td>
               <td><div class="mceField">
	  	         <swe:this property="FormattedHtml" hintText="Field" hintMapType="Hyperlinked Item"/>
	            </div>
	         </td>
         </swe:control> 
      </tr>
   </swe:for-each>
</table>

<!-- wide one-column fields -->
<table datatable="0" summary="">
   <swe:for-each startValue="700" count="5" iteratorName="currentId">
      <swe:control id="swe:currentId" hintMapType="Control">
         <tr>
            <td><span class="mceLabel" align="swe:this.TextAlignmentLabel">
	            <swe:this property="DisplayName" hintText="Label"/>:
	            <swe:this property="RequiredIndicator" hintText="Required"/>
               </span>
            </td>
         </tr>
         <tr>
            <td><span class="mceField" align="swe:this.TextAlignment">
	            <swe:this property="FormattedHtml" hintText="Field" hintMapLabelId="swe:currentId"/>
               </span>
            </td>
         </tr>
      </swe:control>
      <swe:control id="swe:currentId+50">
         <tr>
               <td><class = "Row"><swe:this property="FormattedHtml" hintMapType="Control"/></td>
      	 </tr>
      </swe:control>
      <tr><td><img src="images/spacer.gif" width="1" height="10" alt=""></td></tr>
   </swe:for-each>
</table>
<hr SIZE=4 NOSHADE>
</swe:for-each-row>

