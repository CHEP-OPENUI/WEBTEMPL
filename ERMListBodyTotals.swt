<!-- Begin totals row -->
<table datatable="0" summary="" width="100%">
   <tr>
      <swe:control id="40">
	      <td align="right"><div class="mceLabel" align="swe:this.TextAlignmentLabel">
		      <swe:this property="DisplayName" hintText="Label"/>:
	         </div>
		</td>
		<td><div class="mceField">
	  	      <swe:this property="Total" hintText="Total"/>
            	</div>
            </td>
      </swe:control>
   </tr>
</table>

<hr SIZE=4 NOSHADE>


<swe:for-each-row>
<!------------ Per-record Control Buttons  ------------->
